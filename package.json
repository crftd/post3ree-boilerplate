    "redux-thunk": "^2.1.0",
    "rethinkdb": "^2.2.2",
    "rethinkdbdash": "^2.3.27",
    "style-loader": "^0.13.0",
    "stylelint": "^7.7.1",
    "superagent": "^2.2.0",
    "svg-inline-loader": "^0.7.1",
    "svg-inline-react": "^1.0.1",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "2.1.0-beta.10",
    "xmlhttprequest": "^1.8.0",
    "xss": "^0.2.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "chai": "^4.0.1",
    "chromedriver": "^2.29.0",
    "coveralls": "^2.11.15",
    "nightwatch": "^0.9.16",
    "nightwatch-cucumber": "^7.1.9",
    "phantomjs-prebuilt": "^2.1.14",
    "selenium-server": "^3.4.0",
    "selenium-webdriver": "^3.4.0"
  }
}
