{
  "name": "post3ree-boilerplate",
  "version": "0.3.2",
  "description": "Boilerplate for nodejs universal application using 3REE stack + PostCSS",
  "main": "server.js",
  "scripts": {
    "build:prod": "NODE_ENV=production webpack",
    "db-setup": "babel-node dbSetup.js",
    "db-drop": "babel-node dbDrop.js",
    "test:unit": "jest --config jest-unit-config.json",
    "test:system": "jest --config jest-system-config.json",
    "lint": "eslint client server front test server.js dbSetup.js",
    "start": "NODE_ENV=development node server.babel.js",
    "start:prod": "NODE_ENV=production node server.babel.js",
    "test": "NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test --watch",
    "test:platform:dev": "nightwatch --env chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hex22a/post3ree-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "universal app",
    "react",
    "redux",
    "rethinkdb",
    "jwt",
    "example"
  ],
  "author": "Stdio.Digital",
  "license": "MIT",
  "homepage": "https://github.com/hex22a/post3ree-boilerplate#post3ree-boilerplate",
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|pcss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.0",
    "chance": "^1.0.3",
    "classnames": "^2.2.3",
    "config": "^1.19.0",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.2",
    "cucumber": "^2.3.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "forever": "^0.15.3",
    "history": "^4.3.0",
    "http": "0.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^2.0.0",
    "immutable": "^3.7.6",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.1.9",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "object-assign": "^4.0.1",
    "passport": "^0.3.2",
    "passport-json": "^1.2.0",
    "path": "^0.12.7",
    "postcss-clearfix": "^1.0.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "1.2.2",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "pug": "^2.0.0-beta5",
    "pug-html-loader": "^1.0.8",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.2",
    "react-datepicker": "^0.30.0",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
